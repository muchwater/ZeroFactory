  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch",
    "add:module": "nest g module",
    "add:controller": "nest g module && nest g controller && nest g service",
    "add:service": "nest g module && nest g service",
    "db:up": "docker compose up -d db",
    "db:down": "docker compose down",
    "db:reset": "docker compose down -v && docker compose up -d db && pnpm prisma:reset",
    "db:seed": "node prisma/seed.mjs",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset",
    "prisma:migrate:init": "prisma migrate dev --name init",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "db:setup": "pnpm db:up && pnpm prisma:migrate && pnpm prisma:generate && node prisma/seed.mjs",
    "test": "jest",
    "test:cov": "jest --coverage",
    "reset": "pnpm prisma:reset && rm -rf prisma/migrations && pnpm prisma:migrate:init && pnpm prisma:generate && pnpm db:seed"
  },
